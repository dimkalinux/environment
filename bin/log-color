#! /usr/bin/env bash
REGEX_SYSLOG='[a-z]{3}\s[0-9]{1,2}\s[0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2}\s[a-z0-9]+\s(.*?):\s'
REGEX_PHP='\[[0-9]{1,2}\-[a-z]{3}\-[0-9]{4}\s[0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2}\]\s'
REGEX_ZEND='[0-9]{4}\-[0-9]{2}\-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(\-|\+)[0-9]{2}:[0-9]{2}\s[a-z]+\s\([0-9]\):\s'
REGEX_APACHE='\[[a-z]{3}\s[a-z]{3}\s[0-9]{2}\s[0-9]{2}:[0-9]{2}:[0-9]{2}\s[0-9]{4}\]\s\[[a-z]+\]\s'
ack -i --passthru "^($REGEX_SYSLOG)|($REGEX_PHP)|($REGEX_ZEND)|($REGEX_APACHE)"
